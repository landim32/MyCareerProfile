#php_flag display_errors on
#php_value error_reporting 6143
<IfModule mod_rewrite.c>
RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
#RewriteRule ^(en|pt_BR)/([a-z,A-Z,0-9,_]+).pdf pdf.php?profile=$2&idioma=$1 [QSA,L,END]
RewriteRule ^(en|pt_BR)/([a-z,A-Z,0-9,_]+).pdf pdf.php?profile=$2&idioma=$1 [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f
#RewriteRule ^(en|pt_BR) index.php?idioma=$1 [QSA,L,END]
RewriteRule ^(en|pt_BR) index.php?idioma=$1 [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f
#RewriteRule ^([a-z,A-Z,0-9,\-,_]+) index.php?profile=$1&idioma=pt_BR [QSA,L,END]
RewriteRule ^([a-z,A-Z,0-9,\-,_]+) index.php?profile=$1&idioma=pt_BR [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([a-z,A-Z,0-9,\-,_]+)/(en|pt_BR) index.php?profile=$1&idioma=$2 [QSA,L]

</IfModule>